{
  "name": "aecil/verifactu",
  "description": "Cliente PHP para el servicio Verifactu (TIKE) de la AEAT.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Aecil\\Verifactu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aecil\\Verifactu\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "José Felipe Licea Muñoz",
      "email": "felipelicea1989@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-soap": "*",
    "ext-openssl": "*",
    "simplesoftwareio/simple-qrcode": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5",
    "laravel/pint": "^1.0",
    "orchestra/testbench": "^10.8"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aecil\\Verifactu\\Providers\\VerifactuServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}